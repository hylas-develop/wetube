extends layouts/main
include mixins/videoBlock

block content
    .search__header
        h3 Searching for : #{searchingBy}
    .search__videos
        if videos.length == 0
            h5 No Video Found
        else
            each item in videos
                +videoBlock({
                id : item.id,
                title: item.title,
                description:item.description,
                videoFile:item.fileUrl,
                views:item.views
                })            
        